{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": "5.6",
        "symfony/symfony": "v2.5.0",
        "doctrine/orm": ">=2.4-dev,<2.5-dev",
        "doctrine/doctrine-bundle": "v1.3.0",
        "doctrine/doctrine-fixtures-bundle": "v2.2.1",
        "doctrine/migrations": "v1.5.0",
        "doctrine/doctrine-migrations-bundle": "v1.2.1",
        "gedmo/doctrine-extensions": "v2.3.9",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "friendsofsymfony/rest-bundle": "1.4.1",
        "jms/serializer-bundle": "dev-master",
        "sensio/buzz-bundle": "dev-master",
        "besimple/sso-auth-bundle": "1.0.*@dev",
        "zurb/foundation": "5.4.4",
        "friendsofsymfony/jsrouting-bundle": "v2.0.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zurb/foundation",
                "version": "5.4.4",
                "dist": {
                    "url": "https://github.com/zurb/bower-foundation/archive/5.4.4.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
	"platform": {"php": "5.6"}
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
