{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": "5.6",
        "symfony/symfony": "3.4.0",
        "doctrine/orm": ">=2.5.14",
        "doctrine/doctrine-bundle": ">=1.3.0",
        "doctrine/doctrine-fixtures-bundle": ">=2.2.1",
        "doctrine/migrations": ">=1.5.0",
        "doctrine/doctrine-migrations-bundle": ">=1.2.1",
        "gedmo/doctrine-extensions": ">=2.3.9",
        "twig/extensions": ">=1.0",
        "symfony/assetic-bundle": ">=2.8.2",
        "symfony/swiftmailer-bundle": ">=2.3",
        "symfony/monolog-bundle": ">=2.4",
        "sensio/distribution-bundle": ">=2.3",
        "sensio/framework-extra-bundle": ">=3.0",
        "sensio/generator-bundle": ">=3.1.7",
        "friendsofsymfony/rest-bundle": ">=2.3.1",
        "jms/serializer-bundle": "dev-master",
        "sensio/buzz-bundle": "dev-master",
        "besimple/sso-auth-bundle": "1.0.x-dev",
        "zurb/foundation": ">=5.4.4",
        "friendsofsymfony/jsrouting-bundle": ">=2.2.0",

        "symfony/framework-bundle": ">=2.8.9",
        "symfony/translation": ">=2.8.9",
        "symfony/http-kernel": ">=2.8.8",
        "symfony/http-foundation": ">=2.7",
        "symfony/dependency-injection": ">=2.8",
        "symfony/config": ">=2.8",
        "symfony/debug": ">=2.0",
        "symfony/doctrine-bridge": ">=2.1",
        "symfony/console": ">=3.0.0",
        "symfony/event-dispatcher": ">=3.0.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zurb/foundation",
                "version": "5.4.4",
                "dist": {
                    "url": "https://github.com/zurb/bower-foundation/archive/5.4.4.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
	"platform": {"php": "5.6"}
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
